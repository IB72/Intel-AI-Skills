<Page
    x:Class="SuperResolutionWinMLTestApp_UWP.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <NavigationView x:Name="nvSample" 
                    ItemInvoked="NavigationView_OnItemInvoked" 
                    IsBackButtonVisible="Collapsed"
                    IsBackEnabled="False"
                    PaneDisplayMode="Top"
                    IsSettingsVisible ="False">
        <NavigationView.MenuItems>
            <NavigationViewItem  Content="Super Resolution" Tag="SRWSkillAppPage" FontWeight="Bold" />
            <NavigationViewItem  Content="Skill Information" Tag="SRWSkillInfoPage" FontWeight="Bold" />
        </NavigationView.MenuItems>
        <Frame x:Name="ChildFrame" NavigationFailed="ChildFrame_OnNavigationFailed" />
    </NavigationView>
</Page>
